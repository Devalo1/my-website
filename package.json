{
  "name": "my-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "node scripts/setup-project.cjs",
    "predev": "node scripts/ensure-vite-config.js && node scripts/setup-project.cjs",
    "dev": "vite --base=/my-website/",
    "prebuild": "node scripts/ensure-vite-config.js && node scripts/setup-project.cjs",
    "build": "tsc && vite build",
    "build-netlify": "tsc && vite build --base=/",
    "build-github": "tsc && vite build --base=/my-website/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --base=/my-website/",
    "deploy": "gh-pages -d dist",
    "clean-config": "node scripts/clean-vite-config.js",
    "prebuild-github": "node scripts/setup-project.cjs",
    "prebuild-netlify": "node scripts/setup-project.cjs"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "terser": "^5.17.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
