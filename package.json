{
  "name": "my-website",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --base=/my-website/",
    "check-files": "node scripts/check-missing-files.js",
    "check-resources": "node scripts/resource-checker.js",
    "free-ports": "node scripts/free-ports.js",
    "start-clean": "node scripts/run-dev.js",
    "build": "vite build --base=/my-website/",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node scripts/run-dev.js",
    "build-netlify": "NODE_ENV=production DISABLE_SHARP=true node build-netlify.js",
    "debug": "vite --debug --base=/my-website/",
    "update-deps": "node scripts/update-dependencies.js",
    "fix-package": "node scripts/fix-package-json.js",
    "free-all-ports": "node scripts/free-all-ports.js",
    "create-placeholders": "node scripts/create-placeholder-images.js",
    "convert-images": "node scripts/convert-svg-to-img.js",
    "ensure-images": "node scripts/ensure-images.js",
    "repair-images": "node scripts/repair-images.js",
    "predev": "powershell -ExecutionPolicy Bypass -File scripts/predev.ps1",
    "predev-npm": "npm run ensure-images && npm run repair-images",
    "preinstall": ""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^10.3.10",
    "sharp": "0.33.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "optionalDependencies": {
    "@img/sharp-linux-x64": "^0.33.0",
    "@img/sharp-win32-x64": "^0.33.0",
    "@img/sharp-darwin-x64": "^0.33.0"
  },
  "engines": {
    "node": ">=18"
  }
}
