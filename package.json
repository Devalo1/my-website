{
  "name": "my-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix",
    "preview": "vite preview",
    "prestart": "node scripts/ensure-vite-config.js",
    "prebuild": "node scripts/ensure-vite-config.js",
    "deploy": "git add . && git commit -m \"Update site\" && git push"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.30.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "terser": "^5.17.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  },
  "notes": {
    "/my-website/background.jpg": "referenced in /my-website/background.jpg didn't resolve at build time, it will remain unchanged to be resolved at runtime"
  },
  "with": {
    "github_token": "${{ secrets.GITHUB_TOKEN }}",
    "publish_dir": "dist"
  }
}
